<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @article.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @article.body %>
</p>

<% @article.photos.each do |photo| %>
  <p>
    <%= cl_image_tag(photo.key, gravity: "face", height: 400, width: 600, crop: "fill") %>
  </p>
<% end %>

<%= link_to 'Edit', edit_article_path(@article) %> |
<%= link_to 'Back', articles_path %>
